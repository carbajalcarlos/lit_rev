@article{ ISI:000348600600009,
Author = {Lusch, Robert F. and Nambisan, Satish},
Title = {{SERVICE INNOVATION: A SERVICE-DOMINANT LOGIC PERSPECTIVE}},
Journal = {{MIS QUARTERLY}},
Year = {{2015}},
Volume = {{39}},
